<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sevenorcas.field">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        >

        <activity android:name="com.sevenorcas.field.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sevenorcas.field.graph.GraphActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.sevenorcas.field.result.ResultActivity"
            android:label="@string/app_name">
        </activity>
        <activity
            android:name="com.sevenorcas.field.list.ListActivity"
            android:label="@string/app_name">
        </activity>
    </application>

</manifest>